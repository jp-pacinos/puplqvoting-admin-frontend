  "dependencies": {
    "@reduxjs/toolkit": "^1.5.0",
    "@testing-library/jest-dom": "^5.11.9",
    "@testing-library/react": "^11.2.3",
    "@testing-library/user-event": "^12.6.2",
    "@types/jest": "^26.0.20",
    "@types/lodash": "^4.14.168",
    "@types/node": "^14.14.22",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@types/react-redux": "^7.1.16",
    "@types/react-router-dom": "^5.1.7",
    "@types/react-transition-group": "^4.4.0",
    "apexcharts": "^3.23.1",
    "axios": "^0.21.1",
    "lodash": "^4.17.20",
    "query-string": "^6.13.8",
    "react": "^17.0.1",
    "react-apexcharts": "^1.3.7",
    "react-click-away-listener": "^1.5.0",
    "react-countup": "^4.3.3",
    "react-dom": "^17.0.1",
    "react-helmet-async": "^1.0.7",
    "react-icons": "^4.1.0",
    "react-image-crop": "^8.6.6",
    "react-redux": "^7.2.2",
    "react-router-dom": "^5.2.0",
    "react-scripts": "4.0.1",
    "react-transition-group": "^4.4.1",
    "source-map-explorer": "^2.5.2",
    "typescript": "^4.1.3"
  },
  "scripts": {
    "start": "npm run build:dev:css && react-scripts start",
    "build": "npm run build:css && react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "build:dev:css": "npx tailwindcss build src/assets/css/tailwind.css -o src/app/App.css",
    "build:css": "cross-env NODE_ENV=production npx tailwindcss build src/assets/css/tailwind.css -o src/app/App.css",
    "watch:css": "postcss --watch src/assets/css/tailwind.css -o src/app/App.css",
    "prettier:format": "npx prettier --config .prettierrc --write src/",
    "prettier:check": "npx prettier --config .prettierrc --check src/",
    "analyze": "source-map-explorer 'build/static/js/*.js'"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@fullhuman/postcss-purgecss": "^4.0.0",
    "@types/react-image-crop": "^8.1.2",
    "autoprefixer": "^10.2.3",
    "cross-env": "^7.0.3",
    "postcss": "^8.2.4",
    "postcss-cli": "^8.3.1",
    "tailwindcss": "^2.0.2"
  }
}
